# Production Environment Configuration
API_URL=https://rental-api.melidev.id.vn
API_TIMEOUT=30000

# Authentication
JWT_SECRET_KEY=production-secret-key-change-this
TOKEN_EXPIRY=15m
REFRESH_TOKEN_EXPIRY=7d

# Database
MONGODB_URI=mongodb://production-server:27017/rental-management

# App Configuration
APP_NAME=Rental Management
DEFAULT_LANGUAGE=vi
DEFAULT_CURRENCY=VND

# Feature Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=true

# Production
DEBUG_MODE=false
LOG_LEVEL=error
